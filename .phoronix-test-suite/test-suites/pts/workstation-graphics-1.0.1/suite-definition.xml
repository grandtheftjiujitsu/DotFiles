<?xml version="1.0"?>
<!--Phoronix Test Suite v3.0.0a3 (Iveland) [ http://www.phoronix-test-suite.com/ ]-->
<PhoronixTestSuite>
  <SuiteInformation>
    <Title>Workstation Graphics</Title>
    <Version>1.0.1</Version>
    <TestType>Graphics</TestType>
    <Description>This test suite contains tests that are designed to look at the workstation graphics performance of the test system.</Description>
    <Maintainer>Michael Larabel</Maintainer>
  </SuiteInformation>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>3dsmax</Arguments>
    <Description>3DS Max</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>catia</Arguments>
    <Description>Catia</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>ensight</Arguments>
    <Description>Ensight</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>proe</Arguments>
    <Description>Pro/Engineer</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>maya</Arguments>
    <Description>Maya</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf9</Test>
    <Arguments>light</Arguments>
    <Description>Light</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT catia</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - CATIA</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT ensight</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - EnSight</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT maya</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - Maya</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT proe</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - Pro/Engineer</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT sw</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - SolidWorks</Description>
  </Execute>
  <Execute>
    <Test>pts/specviewperf10</Test>
    <Arguments>$VIDEO_WIDTH $VIDEO_HEIGHT ugnx</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - UGS NX</Description>
  </Execute>
  <Execute>
    <Test>pts/xplane9</Test>
    <Arguments>--fps_test=2 --pref:_x_res_req_ALL=$VIDEO_WIDTH --pref:_y_res_req_ALL=$VIDEO_HEIGHT</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - High Quality</Description>
  </Execute>
  <Execute>
    <Test>pts/xplane9</Test>
    <Arguments>--fps_test=3 --pref:_x_res_req_ALL=$VIDEO_WIDTH --pref:_y_res_req_ALL=$VIDEO_HEIGHT</Arguments>
    <Description>$VIDEO_WIDTH x $VIDEO_HEIGHT - Ultra Quality</Description>
  </Execute>
